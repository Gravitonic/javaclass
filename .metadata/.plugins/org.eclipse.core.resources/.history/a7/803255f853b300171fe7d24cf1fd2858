/*
 * Programmer: Dylan Yang
 * Date: Oct 13 2017
 * Purpose: to use nested loops to print a design using six letters of the alphabet
 */

public class SixLetters {

	public static void main(String[] args) {

		int n;
		char a;

		for (a = 'a'; a <= 'f'; a++) {
			for (n = 0; n <= 6; n++)
			System.out.print(a);
		System.out.println();
		}
	}
}
